{
  "name": "pawbook-client",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm run compile-localization && vite --host",
    "test": "vitest run",
    "watch": "vitest --watch",
    "coverage": "vitest run --coverage",
    "build": "pnpm run compile-localization && paraglide-js compile --project ./project.inlang --outdir ./src/paraglide && vite build",
    "compile-localization": "paraglide-js compile --project ./project.inlang --outdir ./src/lib/paraglide",
    "machine-translate": "inlang machine translate --project project.inlang"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@inlang/cli": "^3.0.0",
    "@inlang/paraglide-js": "2.0.10",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@tsconfig/svelte": "^5.0.4",
    "@vitest/coverage-istanbul": "^3.1.1",
    "esbuild": "^0.25.2",
    "jsdom": "^26.1.0",
    "svelte": "^5.25.3",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "vite": "^6.3.0",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@glidejs/glide": "^3.7.1",
    "@types/node": "^22.13.14",
    "animate.css": "^4.1.1",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "sass": "^1.86.3",
    "svelte-preprocess": "^6.0.3",
    "svelte-spa-router": "^4.0.1",
    "sweetalert2": "^11.17.2",
    "zustand": "^5.0.3"
  }
}
